extends layout

block vars
    - var title = "Player two - Encoding"

block content
  h1 Encoding
  .row
    each vid in processing
      .tile.col-xs-12.col-sm-4.col-md-3(id= "#{vid.name}")
          .video-wrapper
            span.glyphicon.glyphicon-random.action-icon
          div.tile-footer.bg-16(title="#{vid.name}") #{vid.name}
    
  include blocks/nav

block footer

block scripts
  script(type="text/javascript" src="/bower_components/vue/dist/vue.js")
  script(type="text/javascript" src="/js/encode.js")