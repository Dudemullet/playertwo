<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css" href="/js/libs/flat-ui/bootstrap/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="/css/app.css">
    <title>Tenna</title>
  </head>
  <body>
    <div class="container">
      <div id="playerList" class="row video-player"></div>
      <div id="videolist" class="row"></div>
    </div>
    <script id="playerTemplate" type="text/template">
      <video class="col-xs-12 col-sm-12 col-md-12 hide" controls data-vid="<%= name %>" src="<%= stat %>"></video>
    </script>
    <script id="videoicontemplate" type="text/template">
      <div class="title overflow" title="<%= name %>">
        <%= name %>
      </div>
      <div id="<%= name %>" class="video-wrapper">
        <span class="glyphicon glyphicon-play action-icon"></span>
      </div>
      <a class="tile-footer btn" href=" <%= stat %> ">Link</a>
      <button class="tile-footer delete-btn btn" data-name="<%= fileName %>">Delete</button>
    </script>
    <script id="modelData">
      {{- JSON.stringify(models) }}
    </script>
    <script type="text/javascript" src="js/vendor.js"></script>
  </body>
</html>
